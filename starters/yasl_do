#!./yasl

// $> ./yasl_do <value1> <operator> <value2>
// $> ./yasl_do 21 + 42
// $> ./yasl_do 84 ">" 24

!           // drop `argc`
!           // drop `argv[0]`

eval:1      // push `eval(argv[1])` to 1
2 ^         // swap argv[2] and argv[3]
eval:1      // push `eval(argv[3])` to 1
(= !):1     // push argv[2] to 1

1:eval:0    // do operation
print
